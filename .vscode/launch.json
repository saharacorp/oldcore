  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch | Release",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch | Debug",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "start:debug"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Build | Release",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "build"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Build | Debug",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}